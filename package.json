{
  "name": "your-engine",
  "private": true,
  "type": "module",
  "engines": { "node": ">=20.11" },
  "workspaces": ["packages/*", "examples/*"],
  "scripts": {
    "dev": "node scripts/dev-server.mjs",
    "test": "ava",
    "test:visual": "node tools/pixel-compare.mjs",
    "lint": "eslint .",
    "format": "prettier -w .",
    "build": "node scripts/verify-webgpu.mjs && npm -ws run build",
    "docs": "node scripts/gen-docs.mjs",
    "release:check": "node scripts/update-check.mjs",
    "ci": "npm run lint && npm run test && npm run test:visual && npm run build"
  },
  "devDependencies": {
    "ava": "^6.4.1",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "playwright": "^1.47.0",
    "pixelmatch": "^5.3.0",
    "sharp": "^0.33.4",
    "esbuild": "^0.23.1",
    "rollup": "^4.20.0",
    "vite": "^5.4.6",
    "typedoc": "^0.26.6",
    "@babel/parser": "^7.28.3"
  },
  "dependencies": {}
}
